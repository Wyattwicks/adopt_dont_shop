<%= link_to "Pets", "/pets" %>
<%= link_to "Shelters", "/shelters" %>
<%= link_to "Veterinarians", "/veterinarians" %>
<%= link_to "Veterinary Offices", "/veterinary_offices" %>
<%= link_to "Applications", "/applications" %>

<h1><%= @pet.name %></h1>
<h3><%= @pet.age %> years old</h3>
<h3><%= @pet.breed %></h3>
<h3><%= @pet.adoptable %></h3>
<h3><%= @pet.shelter_name %></h3>

<p><%= link_to "Update #{@pet.name}", "pets/#{@pet.id}/edit" %></p>
<p><%= link_to "Delete #{@pet.name}", "/pets/#{@pet.id}", method: :delete %></p>
<p><%= link_to "Adopt #{@pet.name}!!", "/applications/new" %></p>
