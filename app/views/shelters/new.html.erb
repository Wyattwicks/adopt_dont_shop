<%= link_to "Pets", "/pets" %>
<%= link_to "Shelters", "/shelters" %>
<%= link_to "Veterinarians", "/veterinarians" %>
<%= link_to "Veterinary Offices", "/veterinary_offices" %>
<%= link_to "Applications", "/owner_applications" %>

<h1>New Shelter</h1>
<%= form_with url: '/shelters', local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :foster_program %>
  <%= f.check_box :foster_program %>

  <%= f.label :rank %>
  <%= f.number_field :rank %>

  <%= f.submit %>
<% end %>
